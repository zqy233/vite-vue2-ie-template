    "vuex": "^3.6.2",
    "vxe-table": "^3.7.8",
    "xe-utils": "^3.5.20"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.9",
    "@vitejs/plugin-legacy": "5.3.1",
